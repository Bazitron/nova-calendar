{
    "name": "wdelfuego/nova-calendar",
    "description": "A minimalistic event calendar Tool for Laravel's Nova 4.",
    "keywords": [
        "laravel",
        "nova",
        "calendar",
        "event-calendar",
        "events",
        "date"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "wdelfuego/nova-datetime": "^1.0",
        "jenssegers/date": "^4.0"
        
    },
    "autoload": {
        "psr-4": {
            "Wdelfuego\\NovaCalendar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wdelfuego\\NovaCalendar\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
